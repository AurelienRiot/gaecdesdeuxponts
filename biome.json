{
  "formatter": {
    "enabled": true,
    "indentStyle": "tab",
    "lineWidth": 120
  },
  "organizeImports":{
    "enabled":false
  },
  
  "linter": {
    "enabled": true,
    "rules": {
      "correctness":{
"useExhaustiveDependencies":"off"
      },
      "complexity": {
"noUselessFragments":"off"
      },
      "a11y":{
"noSvgWithoutTitle":"off", 
"noAriaHiddenOnFocusable":"off"
      },
      "suspicious": {
"noArrayIndexKey":"off",
"noExplicitAny":"off"
      },
      "style": {
        "useTemplate": "off",
        "noUnusedTemplateLiteral":"off", 
        "useSelfClosingElements":"off"
      }
    }
  }
}